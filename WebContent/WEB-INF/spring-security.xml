        <intercept-url pattern="/dwr/call/plaincall/WatchListDwr.*.dwr" access="hasAnyRole('ROLE_ADMIN', 'ROLE_USER')" />

        <!-- DataPointDetailsDwr -->
        <intercept-url pattern="/dwr/call/plaincall/DataPointDetailsDwr.*.dwr" access="hasAnyRole('ROLE_ADMIN', 'ROLE_USER')" />

        <!-- DataSourceEditDwr -->
